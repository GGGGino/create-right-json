{
  "validations": [{
    "name": "allFields",
    "type": "object",
    "relation": {
      "allOf": [{
        "not": {
          "{nome}": null
        }
      }, {
        "greater": ["{nome}", "{dataNascita}"]
      }, {
        "anyOf": [{
          "allOf": [{
            "{nome}": true
          }, {
            "not": {
              "{dataNascita}": null
            }
          }]
        }, {
          "allOf": [{
            "{dataNascita}": true
          }, {
            "not": {
              "{comuneNascita}": null
            }
          }, {
            "not": {
              "{books_nome}": null
            }
          }]
        }]
      }]
    }
  }]
}